{
  "name": "ai-content-studio-dashboard",
  "version": "2.0.0",
  "description": "AI Content Studio - Modern Web Dashboard for automated video content creation",
  "main": "launch-dashboard.js",
  "scripts": {
    "start": "node launch-dashboard.js",
    "dev": "nodemon launch-dashboard.js",
    "dashboard": "node launch-dashboard.js",
    "test": "node test-ai-content-studio.js",
    "test:components": "node test-components.js",
    "test:enterprise": "node test-enterprise-pipeline.js",
    "health": "curl http://localhost:3001/health",
    "stop": "pkill -f launch-dashboard.js"
  },
  "keywords": [
    "ai",
    "content-creation",
    "video-automation",
    "youtube",
    "script-generation",
    "thumbnail-creation",
    "seo-optimization",
    "dashboard",
    "websocket",
    "real-time",
    "enterprise",
    "saas",
    "artificial-intelligence",
    "automation"
  ],
  "author": {
    "name": "Thiago Batista Teixeira",
    "email": "thiago.teixeira51@etec.sp.gov.br",
    "url": "https://github.com/EngThi"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EngThi/novo3.git"
  },
  "bugs": {
    "url": "https://github.com/EngThi/novo3/issues"
  },
  "homepage": "https://github.com/EngThi/novo3#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "rate-limiter-flexible": "^3.0.8",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "optionalDependencies": {
    "sharp": "^0.32.6",
    "canvas": "^2.11.2",
    "ffmpeg-static": "^5.2.0"
  },
  "config": {
    "port": 3001,
    "host": "localhost",
    "env": "development"
  },
  "features": {
    "aiScriptGeneration": {
      "enabled": true,
      "styles": ["educativo", "entretenimento", "news", "tutorial"],
      "maxTokens": 2048,
      "quality": "enterprise"
    },
    "smartThumbnailCreation": {
      "enabled": true,
      "templates": ["tech_modern", "viral_impact", "professional_clean", "education_friendly", "news_urgent"],
      "abTesting": true,
      "ctrPrediction": true
    },
    "enterprisePipeline": {
      "enabled": true,
      "strategies": ["gcp-free", "premium", "quality", "speed"],
      "jobQueue": true,
      "metrics": true
    },
    "webDashboard": {
      "enabled": true,
      "realTimeUpdates": true,
      "websocket": true,
      "analytics": true,
      "responsive": true
    },
    "seoOptimization": {
      "enabled": true,
      "keywordResearch": true,
      "trendAnalysis": true,
      "performancePrediction": true
    }
  },
  "performance": {
    "avgScriptGeneration": "2-5s",
    "avgThumbnailGeneration": "3-7s",
    "avgVideoGeneration": "30-120s",
    "concurrentJobs": 3,
    "cacheEnabled": true,
    "compressionEnabled": true
  },
  "monetization": {
    "valuePerVideo": "$50-200",
    "speedImprovement": "10x faster",
    "qualityImprovement": "+40% CTR",
    "roi": "300-500%",
    "targetMarket": "Content creators, Marketing agencies, YouTubers"
  },
  "api": {
    "version": "2.0",
    "endpoints": {
      "/health": "System health check",
      "/api/content/script": "AI script generation",
      "/api/content/thumbnail": "Smart thumbnail creation",
      "/api/content/video": "Complete video generation",
      "/api/analytics/overview": "Performance analytics",
      "/api/system/status": "System status and metrics"
    },
    "rateLimit": "100 requests/minute",
    "authentication": "Optional JWT",
    "cors": "Enabled"
  },
  "deployment": {
    "docker": {
      "enabled": false,
      "image": "ai-content-studio:latest"
    },
    "cloud": {
      "platforms": ["Heroku", "AWS", "Google Cloud", "Azure"],
      "scaling": "Horizontal",
      "monitoring": "Built-in metrics"
    },
    "environment": {
      "development": {
        "port": 3001,
        "debug": true,
        "cache": false
      },
      "production": {
        "port": 80,
        "debug": false,
        "cache": true,
        "compression": true,
        "security": true
      }
    }
  },
  "support": {
    "documentation": "README.md",
    "examples": "examples/",
    "community": "GitHub Issues",
    "enterprise": "thiago.teixeira51@etec.sp.gov.br"
  },
  "changelog": {
    "2.0.0": "Complete AI Content Studio with Web Dashboard",
    "1.0.0": "Basic pipeline implementation"
  }
}