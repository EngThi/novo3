{
  "name": "novo3-unified-pipeline",
  "version": "3.0.0",
  "description": "Pipeline Unificado para Automação de Vídeos com IA - Sistema Inteligente e Otimizado",
  "main": "pipeline-unified.js",
  "scripts": {
    "start": "node pipeline-unified.js",
    "dev": "node pipeline-unified.js --debug",
    "free": "node pipeline-unified.js --mode=free",
    "premium": "node pipeline-unified.js --mode=premium",
    "premium-v2": "node pipeline-unified.js --mode=premium-v2",
    "test": "node quick-test.js",
    "voice": "node voice-selector.js",
    "setup": "bash setup-gcp-free.sh",
    "logs": "tail -f novo/logs/execution_$(date +%Y-%m-%d).json",
    "cleanup": "rm -rf novo/temp/* novo/cache/* novo/logs/*",
    "stats": "node -e \"const p = require('./pipeline-unified'); new p().getMetrics().then(console.log)\";"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EngThi/novo3.git"
  },
  "keywords": [
    "video-automation",
    "ai-content-creation",
    "tts", 
    "image-generation",
    "gemini",
    "nano-banana",
    "huggingface",
    "youtube-automation",
    "brazilian-content"
  ],
  "author": "Thiago Batista Teixeira",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@huggingface/inference": "^2.8.0",
    "@fal-ai/client": "^1.0.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "googleapis": "^144.0.0",
    "sharp": "^0.33.5",
    "fluent-ffmpeg": "^2.1.3",
    "canvas": "^2.11.2",
    "node-cron": "^3.0.3",
    "winston": "^3.14.2",
    "uuid": "^10.0.0",
    "mime-types": "^2.1.35",
    "cheerio": "^1.0.0",
    "puppeteer": "^23.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "jest": "^29.7.0",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3"
  },
  "optionalDependencies": {
    "editly": "^0.13.2",
    "remotion": "^4.0.225",
    "playwright": "^1.47.2"
  },
  "peerDependencies": {
    "python3": ">=3.8.0",
    "ffmpeg": ">=4.0.0"
  },
  "config": {
    "pipeline_mode": "auto",
    "default_voice": "Kore",
    "default_language": "pt-BR",
    "cache_enabled": true,
    "auto_cleanup": true
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/EngThi"
  },
  "bin": {
    "novo3": "./pipeline-unified.js",
    "novo3-voice": "./voice-selector.js"
  },
  "files": [
    "pipeline-unified.js",
    "voice-selector.js",
    "modules/",
    "config-premium-v2.js",
    "stateManager.js",
    "README-UNIFIED.md"
  ]
}